<demo-block title="基础用法">
  <van-cell-group wid="1">
    <van-switch-cell
      title="标题"
      checked="{{ checked }}"
      bind:change="onChange"
      pid="1"
    />
  </van-cell-group>
</demo-block>

<demo-block title="禁用状态">
  <van-cell-group wid="2">
