      disabled
      title="标题"
      checked="{{ checked }}"
      bind:change="onChange"
      pid="2"
    />
  </van-cell-group>
</demo-block>

<demo-block title="加载状态">
  <van-cell-group wid="3">
    <van-switch-cell
      loading
      title="标题"
      checked="{{ checked }}"
      bind:change="onChange"
      pid="3"
    />
  </van-cell-group>
</demo-block>
